<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"> <!-- bean definitions here -->
	<bean id="name" class="java.lang.String">
        <constructor-arg value="Spring Framework Nadia"/> 
    </bean>

    <bean id="age" class="java.lang.Integer">
        <constructor-arg value="33"/>
    </bean>
    <!-- <context:component-scan base-package="com.nadia.gaming, com.nadia.gamingSpring"/> -->

    <bean id="pacMan" class="com.nadia.gaming.PacMan"/>
    <bean id="gameRunner" class="com.nadia.gaming.GameRunner">
        <constructor-arg ref="pacMan"/>
    </bean>

 	
</beans>